# Coolify API
COOLIFY_API_URL=http://localhost:8000/api/v1
COOLIFY_API_TOKEN=your_api_token_here

# SSH Keys (Coolify's keys)
SSH_KEYS_PATH=/data/coolify/ssh/keys

# Temp directory for transfers
TEMP_DIR=/tmp/coolify-mover

